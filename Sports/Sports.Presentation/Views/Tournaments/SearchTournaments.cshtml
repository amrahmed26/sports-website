
@{
    ViewData["Title"] = "SearchTournaments";
    Layout = "~/Views/Shared/_LayoutWebsite.cshtml";
}

<div class="form-group row">
    <label class="col-lg-4 col-form-label" for="val-skill">
        Tournaments <span class="text-danger">*</span>
    </label>
    <div class="col-lg-6">
        <input type="text" class="form-control" id="Key" name="val-username" placeholder="Enter words" required>


    </div>
</div>
<div class="form-group row">
    <label class="col-lg-4 col-form-label" for="val-skill">
    </label>
    <div class="col-lg-6">
        <div id="submitKey" class="btn btn-primary">Search</div>


    </div>
</div>
<table class="table table-striped table-bordered " id="Data">
    <thead>
        <tr>
            <th>Name</th>
            <th>Logo </th>
            <th> </th>

        </tr>
    </thead>
    <tbody>
    </tbody>
    <tfoot>
        <tr>
            <th>Name</th>
            <th>Logo </th>
            <th> </th>

        </tr>
    </tfoot>
</table>

@section Scripts{
    <script type="module" src="~/js/jsviews/SearchTournaments.js"></script>
